    "mainEntity" : "pli-tv-pm:PatimokkhaRule_pli-tv-bi-pm-pc50",
    "text" : "Yā pana bhikkhunī tiracchānavijjaṁ vāceyya, pācittiyaṁ.",
    "comment" : "Bhikkhunī Pācittiya 50. Tiracchānavijjāvācanasikkhāpadaṁ",
    "label" : "Bhikkhunī Pācittiya 50"
  }, {
    "@id" : "pli-tv-pm:VinayaSegment_pli-tv-bi-pm-pc51",
    "@type" : "http://samanasara.org/ontology/VinayaStudiesOntology#VinayaSegment",
    "about" : [ "http://samanasara.org/VinayaCore#VinayaTradition_PaliTextTheravada", "http://samanasara.org/VinayaCore#VinayaOffenseClass_Pacittiya" ],
    "inLanguage" : "pli",
    "isPartOf" : "http://samanasara.org/VinayaCore#VinayaText_Mahāsaṅgīti_Tipiṭaka_Buddhavasse_2500",
    "mainEntity" : "pli-tv-pm:PatimokkhaRule_pli-tv-bi-pm-pc51",
    "text" : "Yā pana bhikkhunī jānaṁ sabhikkhukaṁ ārāmaṁ anāpucchā paviseyya, pācittiyaṁ.",
    "comment" : "Bhikkhunī Pācittiya 51. Ārāmapavisanasikkhāpadaṁ",
    "label" : "Bhikkhunī Pācittiya 51"
  }, {
    "@id" : "pli-tv-pm:VinayaSegment_pli-tv-bi-pm-pc52",
    "@type" : "http://samanasara.org/ontology/VinayaStudiesOntology#VinayaSegment",
    "about" : [ "http://samanasara.org/VinayaCore#VinayaTradition_PaliTextTheravada", "http://samanasara.org/VinayaCore#VinayaOffenseClass_Pacittiya" ],
