  }, {
    "@id" : "pli-tv-pm:VinayaSegment_pli-tv-bu-pm-as6",
    "@type" : "http://samanasara.org/ontology/VinayaStudiesOntology#VinayaSegment",
    "about" : [ "http://samanasara.org/VinayaCore#VinayaTradition_PaliTextTheravada", "http://samanasara.org/VinayaCore#VinayaOffenseClass_Adhikaranasamatha" ],
    "inLanguage" : "pli",
    "isPartOf" : "http://samanasara.org/VinayaCore#VinayaText_Mahāsaṅgīti_Tipiṭaka_Buddhavasse_2500",
    "mainEntity" : "pli-tv-pm:PatimokkhaRule_pli-tv-bu-pm-as6",
