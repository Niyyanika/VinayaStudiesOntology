    "isPartOf" : "http://samanasara.org/VinayaCore#VinayaText_Mahāsaṅgīti_Tipiṭaka_Buddhavasse_2500",
    "mainEntity" : "pli-tv-pm:PatimokkhaRule_pli-tv-bi-pm-pc52",
    "text" : "Yā pana bhikkhunī bhikkhuṁ akkoseyya vā paribhāseyya vā, pācittiyaṁ.",
    "comment" : "Bhikkhunī Pācittiya 52. Bhikkhuakkosanasikkhāpadaṁ",
    "label" : "Bhikkhunī Pācittiya 52"
  }, {
    "@id" : "pli-tv-pm:VinayaSegment_pli-tv-bi-pm-pc53",
    "@type" : "http://samanasara.org/ontology/VinayaStudiesOntology#VinayaSegment",
    "about" : [ "http://samanasara.org/VinayaCore#VinayaTradition_PaliTextTheravada", "http://samanasara.org/VinayaCore#VinayaOffenseClass_Pacittiya" ],
    "inLanguage" : "pli",
    "isPartOf" : "http://samanasara.org/VinayaCore#VinayaText_Mahāsaṅgīti_Tipiṭaka_Buddhavasse_2500",
    "mainEntity" : "pli-tv-pm:PatimokkhaRule_pli-tv-bi-pm-pc53",
    "text" : "Yā pana bhikkhunī caṇḍīkatā gaṇaṁ paribhāseyya, pācittiyaṁ.",
    "comment" : "Bhikkhunī Pācittiya 53. Gaṇaparibhāsanasikkhāpadaṁ",
    "label" : "Bhikkhunī Pācittiya 53"
  }, {
    "@id" : "pli-tv-pm:VinayaSegment_pli-tv-bi-pm-pc54",
    "@type" : "http://samanasara.org/ontology/VinayaStudiesOntology#VinayaSegment",
    "about" : [ "http://samanasara.org/VinayaCore#VinayaTradition_PaliTextTheravada", "http://samanasara.org/VinayaCore#VinayaOffenseClass_Pacittiya" ],
    "inLanguage" : "pli",
    "isPartOf" : "http://samanasara.org/VinayaCore#VinayaText_Mahāsaṅgīti_Tipiṭaka_Buddhavasse_2500",
    "mainEntity" : "pli-tv-pm:PatimokkhaRule_pli-tv-bi-pm-pc54",
    "text" : "Yā pana bhikkhunī nimantitā vā pavāritā vā khādanīyaṁ vā bhojanīyaṁ vā khādeyya vā bhuñjeyya vā, pācittiyaṁ.",
    "comment" : "Bhikkhunī Pācittiya 54. Pavāritasikkhāpadaṁ",
    "label" : "Bhikkhunī Pācittiya 54"
  }, {
    "@id" : "pli-tv-pm:VinayaSegment_pli-tv-bi-pm-pc55",
    "@type" : "http://samanasara.org/ontology/VinayaStudiesOntology#VinayaSegment",
    "about" : [ "http://samanasara.org/VinayaCore#VinayaTradition_PaliTextTheravada", "http://samanasara.org/VinayaCore#VinayaOffenseClass_Pacittiya" ],
    "inLanguage" : "pli",
    "isPartOf" : "http://samanasara.org/VinayaCore#VinayaText_Mahāsaṅgīti_Tipiṭaka_Buddhavasse_2500",
    "mainEntity" : "pli-tv-pm:PatimokkhaRule_pli-tv-bi-pm-pc55",
    "text" : "Yā pana bhikkhunī kulamaccharinī assa, pācittiyaṁ.",
    "comment" : "Bhikkhunī Pācittiya 55. Kulamaccharinīsikkhāpadaṁ",
    "label" : "Bhikkhunī Pācittiya 55"
  }, {
    "@id" : "pli-tv-pm:VinayaSegment_pli-tv-bi-pm-pc56",
    "@type" : "http://samanasara.org/ontology/VinayaStudiesOntology#VinayaSegment",
    "about" : [ "http://samanasara.org/VinayaCore#VinayaTradition_PaliTextTheravada", "http://samanasara.org/VinayaCore#VinayaOffenseClass_Pacittiya" ],
    "inLanguage" : "pli",
    "isPartOf" : "http://samanasara.org/VinayaCore#VinayaText_Mahāsaṅgīti_Tipiṭaka_Buddhavasse_2500",
