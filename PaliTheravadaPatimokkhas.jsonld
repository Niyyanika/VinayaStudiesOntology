    "isPartOf" : "http://samanasara.org/VinayaCore#VinayaText_Mahāsaṅgīti_Tipiṭaka_Buddhavasse_2500",
    "mainEntity" : "pli-tv-pm:PatimokkhaRule_pli-tv-bi-pm-pc100",
    "text" : "Yā pana bhikkhunī anupasampannaṁ padaso dhammaṁ vāceyya, pācittiyaṁ.",
    "comment" : "Bhikkhunī Pācittiya 100. Padasodhammasikkhāpadaṁ",
    "label" : "Bhikkhunī Pācittiya 100"
  }, {
    "@id" : "pli-tv-pm:VinayaSegment_pli-tv-bi-pm-pc101",
    "@type" : "http://samanasara.org/ontology/VinayaStudiesOntology#VinayaSegment",
    "about" : [ "http://samanasara.org/VinayaCore#VinayaTradition_PaliTextTheravada", "http://samanasara.org/VinayaCore#VinayaOffenseClass_Pacittiya" ],
    "inLanguage" : "pli",
    "isPartOf" : "http://samanasara.org/VinayaCore#VinayaText_Mahāsaṅgīti_Tipiṭaka_Buddhavasse_2500",
    "mainEntity" : "pli-tv-pm:PatimokkhaRule_pli-tv-bi-pm-pc101",
    "text" : "Yā pana bhikkhunī anupasampannāya uttaridirattatirattaṁ sahaseyyaṁ kappeyya, pācittiyaṁ.",
    "comment" : "Bhikkhunī Pācittiya 101. Paṭhamasahaseyyasikkhāpadaṁ",
    "label" : "Bhikkhunī Pācittiya 101"
  }, {
    "@id" : "pli-tv-pm:VinayaSegment_pli-tv-bi-pm-pc102",
    "@type" : "http://samanasara.org/ontology/VinayaStudiesOntology#VinayaSegment",
